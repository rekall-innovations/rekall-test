#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/android/output/TestSearch /home/scudette/projects/rekall-test/android/TestSearch
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/android/output/TestSearch /home/scudette/projects/rekall-test/android/TestSearch
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/android/output/TestSearch /home/scudette/projects/rekall-test/android/TestSearch
elif [ "$1" == "run" ]; then
   rekall  -v --profile '/home/scudette/projects/rekall-test/android/Evo4G.json' --cache_dir '/home/scudette/projects/rekall-test/android/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/android/Evo4GRodeo.aff4' --name_resolution_strategies 'Module' 'Symbol' - search 'select * from pslist where (proc.pid == 1)'
else
   less /home/scudette/projects/rekall-test/android/output/TestSearch
fi
exit 0
