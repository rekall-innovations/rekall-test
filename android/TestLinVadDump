#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/scudette/projects/rekall-test/android/output/TestLinVadDump /Users/scudette/projects/rekall-test/android/TestLinVadDump
elif [ "$1" == "meld" ]; then
   meld /Users/scudette/projects/rekall-test/android/output/TestLinVadDump /Users/scudette/projects/rekall-test/android/TestLinVadDump
elif [ "$1" == "diff" ]; then
   diff /Users/scudette/projects/rekall-test/android/output/TestLinVadDump /Users/scudette/projects/rekall-test/android/TestLinVadDump
elif [ "$1" == "run" ]; then
   rekall  -v --profile '/Users/scudette/projects/rekall-test/android/Evo4G.json' --cache_dir '/Users/scudette/projects/rekall-test/android/../profile_cache/' --format 'test' --filename '/Users/scudette/projects/rekall-test/android/Evo4GRodeo.aff4' --name_resolution_strategies 'Module' 'Symbol' - vaddump --proc_regex sh --dump_dir /var/folders/y8/670pn1dx55x01z5fs_6r2gnh003jp1/T/tmp0Dlo3v/TestLinVadDumpThread-7
else
   less /Users/scudette/projects/rekall-test/android/output/TestLinVadDump
fi
exit 0
