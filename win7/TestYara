#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/win7/output/TestYara /home/scudette/projects/rekall-test/win7/TestYara
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/win7/output/TestYara /home/scudette/projects/rekall-test/win7/TestYara
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/win7/output/TestYara /home/scudette/projects/rekall-test/win7/TestYara
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/scudette/projects/rekall-test/win7/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/win7/win7.elf.E01' --name_resolution_strategies 'Module' 'Symbol' - yarascan --string Microsoft --hits 10
else
   less /home/scudette/projects/rekall-test/win7/output/TestYara
fi
exit 0
