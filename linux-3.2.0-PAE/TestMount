#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/mic/projects/rekall-test/linux-3.2.0-PAE/output/TestMount /home/mic/projects/rekall-test/linux-3.2.0-PAE/TestMount
elif [ "$1" == "meld" ]; then
   meld /home/mic/projects/rekall-test/linux-3.2.0-PAE/output/TestMount /home/mic/projects/rekall-test/linux-3.2.0-PAE/TestMount
elif [ "$1" == "diff" ]; then
   diff /home/mic/projects/rekall-test/linux-3.2.0-PAE/output/TestMount /home/mic/projects/rekall-test/linux-3.2.0-PAE/TestMount
elif [ "$1" == "run" ]; then
   rekall  -v --filename '/home/mic/projects/rekall-test/linux-3.2.0-PAE/Linux-3.2.0-4-686-pae.E01' --cache_dir '/home/mic/projects/rekall-test/linux-3.2.0-PAE/../profile_cache/' --cache 'memory' --format 'test' - mount
else
   less /home/mic/projects/rekall-test/linux-3.2.0-PAE/output/TestMount
fi
exit 0
