   cp /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestBanner /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestBanner
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestBanner /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestBanner
