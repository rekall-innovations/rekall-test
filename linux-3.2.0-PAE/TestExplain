#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestExplain /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestExplain
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestExplain /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestExplain
