#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestCheckTaskFops /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestCheckTaskFops
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestCheckTaskFops /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestCheckTaskFops
elif [ "$1" == "diff" ]; then
