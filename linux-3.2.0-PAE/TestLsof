#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/mic/projects/rekall-test/linux-3.2.0-PAE/output/TestLsof /home/mic/projects/rekall-test/linux-3.2.0-PAE/TestLsof
elif [ "$1" == "meld" ]; then
   meld /home/mic/projects/rekall-test/linux-3.2.0-PAE/output/TestLsof /home/mic/projects/rekall-test/linux-3.2.0-PAE/TestLsof
elif [ "$1" == "diff" ]; then
   diff /home/mic/projects/rekall-test/linux-3.2.0-PAE/output/TestLsof /home/mic/projects/rekall-test/linux-3.2.0-PAE/TestLsof
elif [ "$1" == "run" ]; then
   rekall  -v --cache_dir '/home/mic/projects/rekall-test/linux-3.2.0-PAE/../profile_cache/' --format 'test' --filename '/home/mic/projects/rekall-test/linux-3.2.0-PAE/Linux-3.2.0-4-686-pae.E01' --cache 'memory' - lsof --proc_regex bash
else
   less /home/mic/projects/rekall-test/linux-3.2.0-PAE/output/TestLsof
fi
exit 0
