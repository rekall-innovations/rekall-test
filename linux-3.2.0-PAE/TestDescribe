#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestDescribe /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestDescribe
