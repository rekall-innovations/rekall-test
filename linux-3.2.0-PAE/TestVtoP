#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestVtoP /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestVtoP
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestVtoP /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestVtoP
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestVtoP /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestVtoP
elif [ "$1" == "run" ]; then
   rekall  -v --cache_dir '/home/scudette/projects/rekall-test/linux-3.2.0-PAE/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/linux-3.2.0-PAE/Linux-3.2.0-4-686-pae.E01' - vtop --pid 2362 - 4119948992
else
   less /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestVtoP
fi
exit 0
