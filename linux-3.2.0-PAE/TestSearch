#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestSearch /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestSearch
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestSearch /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestSearch
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestSearch /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestSearch
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/scudette/projects/rekall-test/linux-3.2.0-PAE/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/linux-3.2.0-PAE/Linux-3.2.0-4-686-pae.E01' - search 'select * from pslist() where (proc.pid == 1)'
else
   less /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestSearch
fi
exit 0
