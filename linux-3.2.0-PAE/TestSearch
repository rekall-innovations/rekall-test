#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/adamsh/Projects/test/linux-3.2.0-PAE/output/TestSearch /Users/adamsh/Projects/test/linux-3.2.0-PAE/TestSearch
elif [ "$1" == "meld" ]; then
   meld /Users/adamsh/Projects/test/linux-3.2.0-PAE/output/TestSearch /Users/adamsh/Projects/test/linux-3.2.0-PAE/TestSearch
elif [ "$1" == "diff" ]; then
   diff /Users/adamsh/Projects/test/linux-3.2.0-PAE/output/TestSearch /Users/adamsh/Projects/test/linux-3.2.0-PAE/TestSearch
