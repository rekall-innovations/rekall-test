#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestMcat /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestMcat
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestMcat /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestMcat
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestMcat /home/scudette/projects/rekall-test/linux-3.2.0-PAE/TestMcat
elif [ "$1" == "run" ]; then
   rekall  -v --cache_dir '/home/scudette/projects/rekall-test/linux-3.2.0-PAE/../profile_cache/' --cache 'memory' --format 'test' --filename '/home/scudette/projects/rekall-test/linux-3.2.0-PAE/Linux-3.2.0-4-686-pae.E01' - mcat /etc/passwd --dump_dir /tmp/tmpLp49nU/TestMcatThread-3
else
   less /home/scudette/projects/rekall-test/linux-3.2.0-PAE/output/TestMcat
fi
exit 0
