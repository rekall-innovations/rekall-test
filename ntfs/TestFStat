#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/scudette/projects/rekall-test/ntfs/output/TestFStat /Users/scudette/projects/rekall-test/ntfs/TestFStat
elif [ "$1" == "meld" ]; then
   meld /Users/scudette/projects/rekall-test/ntfs/output/TestFStat /Users/scudette/projects/rekall-test/ntfs/TestFStat
