#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/ntfs/output/TestFStat /home/scudette/projects/rekall-test/ntfs/TestFStat
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/ntfs/output/TestFStat /home/scudette/projects/rekall-test/ntfs/TestFStat
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/ntfs/output/TestFStat /home/scudette/projects/rekall-test/ntfs/TestFStat
elif [ "$1" == "run" ]; then
   rekall  --profile 'ntfs' --renderer 'test' --filename '/home/scudette/projects/rekall-test/ntfs/ntfs1-gen1.E01' - fstat RAW/20076517123273.pdf
else
   less /home/scudette/projects/rekall-test/ntfs/output/TestFStat
fi
exit 0
