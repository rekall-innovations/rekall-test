#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/mic/projects/rekall-test/ntfs/output/TestDT /home/mic/projects/rekall-test/ntfs/TestDT
elif [ "$1" == "meld" ]; then
   meld /home/mic/projects/rekall-test/ntfs/output/TestDT /home/mic/projects/rekall-test/ntfs/TestDT
elif [ "$1" == "diff" ]; then
   diff /home/mic/projects/rekall-test/ntfs/output/TestDT /home/mic/projects/rekall-test/ntfs/TestDT
elif [ "$1" == "run" ]; then
   rekall  -v --filename '/home/mic/projects/rekall-test/ntfs/ntfs1-gen1.E01' --cache 'memory' --profile 'ntfs' --cache_dir '/home/mic/projects/rekall-test/ntfs/../profile_cache/' --format 'test' - dt MFT_ENTRY
else
   less /home/mic/projects/rekall-test/ntfs/output/TestDT
fi
exit 0
