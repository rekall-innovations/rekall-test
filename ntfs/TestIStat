#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/scudette/projects/rekall-test/ntfs/output/TestIStat /Users/scudette/projects/rekall-test/ntfs/TestIStat
elif [ "$1" == "meld" ]; then
   meld /Users/scudette/projects/rekall-test/ntfs/output/TestIStat /Users/scudette/projects/rekall-test/ntfs/TestIStat
elif [ "$1" == "diff" ]; then
   diff /Users/scudette/projects/rekall-test/ntfs/output/TestIStat /Users/scudette/projects/rekall-test/ntfs/TestIStat
elif [ "$1" == "run" ]; then
   rekall  -v --profile 'ntfs' --cache_dir '/Users/scudette/projects/rekall-test/ntfs/../profile_cache/' --format 'test' --filename '/Users/scudette/projects/rekall-test/ntfs/ntfs1-gen1.E01' - istat 40
else
   less /Users/scudette/projects/rekall-test/ntfs/output/TestIStat
fi
exit 0
