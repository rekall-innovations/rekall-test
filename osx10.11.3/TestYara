#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/osx10.11.3/output/TestYara /home/scudette/projects/rekall-test/osx10.11.3/TestYara
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/osx10.11.3/output/TestYara /home/scudette/projects/rekall-test/osx10.11.3/TestYara
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/osx10.11.3/output/TestYara /home/scudette/projects/rekall-test/osx10.11.3/TestYara
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/scudette/projects/rekall-test/osx10.11.3/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/osx10.11.3/10.11.3.aff4' --name_resolution_strategies 'Module' 'Symbol' - yarascan --string talagent --hits 10
else
   less /home/scudette/projects/rekall-test/osx10.11.3/output/TestYara
fi
exit 0
