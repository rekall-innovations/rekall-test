#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/adamsh/Projects/rekall-test/osx10.9.5/output/TestDarwinListFiles /Users/adamsh/Projects/rekall-test/osx10.9.5/TestDarwinListFiles
elif [ "$1" == "meld" ]; then
   meld /Users/adamsh/Projects/rekall-test/osx10.9.5/output/TestDarwinListFiles /Users/adamsh/Projects/rekall-test/osx10.9.5/TestDarwinListFiles
