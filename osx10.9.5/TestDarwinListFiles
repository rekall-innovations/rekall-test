   rekall  --renderer 'test' --filename '/Users/adamsh/Projects/rekall-test/osx10.9.5/vmem.e01' --name_resolution_strategies 'Module' 'Symbol' - list_files
else
   less /Users/adamsh/Projects/rekall-test/osx10.9.5/output/TestDarwinListFiles
fi
exit 0
