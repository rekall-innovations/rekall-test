   rekall  --renderer 'test' --filename '/Users/adamsh/Projects/rekall-test/osx10.9.5/vmem.e01' --name_resolution_strategies 'Module,Symbol' - maps --proc_regex bash
else
   less /Users/adamsh/Projects/rekall-test/osx10.9.5/output/TestProcMaps
fi
exit 0
