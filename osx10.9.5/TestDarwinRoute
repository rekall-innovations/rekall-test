#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/osx10.9.5/output/TestDarwinRoute /home/scudette/projects/rekall-test/osx10.9.5/TestDarwinRoute
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/osx10.9.5/output/TestDarwinRoute /home/scudette/projects/rekall-test/osx10.9.5/TestDarwinRoute
elif [ "$1" == "diff" ]; then
