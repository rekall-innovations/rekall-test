#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/scudette/projects/rekall-test/osx10.9.5/output/TestYara /Users/scudette/projects/rekall-test/osx10.9.5/TestYara
elif [ "$1" == "meld" ]; then
   meld /Users/scudette/projects/rekall-test/osx10.9.5/output/TestYara /Users/scudette/projects/rekall-test/osx10.9.5/TestYara
elif [ "$1" == "diff" ]; then
   diff /Users/scudette/projects/rekall-test/osx10.9.5/output/TestYara /Users/scudette/projects/rekall-test/osx10.9.5/TestYara
elif [ "$1" == "run" ]; then
   rekall  -v --cache_dir '/Users/scudette/projects/rekall-test/osx10.9.5/../profile_cache/' --format 'test' --filename '/Users/scudette/projects/rekall-test/osx10.9.5/vmem.e01' --name_resolution_strategies 'Module' 'Symbol' - yarascan --string talagent
else
   less /Users/scudette/projects/rekall-test/osx10.9.5/output/TestYara
fi
exit 0
