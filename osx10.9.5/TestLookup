#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/adam/src/rekall-test/osx10.9.5/output/TestLookup /home/adam/src/rekall-test/osx10.9.5/TestLookup
elif [ "$1" == "meld" ]; then
   meld /home/adam/src/rekall-test/osx10.9.5/output/TestLookup /home/adam/src/rekall-test/osx10.9.5/TestLookup
elif [ "$1" == "diff" ]; then
   diff /home/adam/src/rekall-test/osx10.9.5/output/TestLookup /home/adam/src/rekall-test/osx10.9.5/TestLookup
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/adam/src/rekall-test/osx10.9.5/../profile_cache/' --format 'test' --filename '/home/adam/src/rekall-test/osx10.9.5/vmem.e01' --name_resolution_strategies 'Module' 'Symbol' - lookup '_PE_state' 'PE_state'
else
   less /home/adam/src/rekall-test/osx10.9.5/output/TestLookup
fi
exit 0
