#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/osx10.9.5/output/TestExplain /home/scudette/projects/rekall-test/osx10.9.5/TestExplain
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/osx10.9.5/output/TestExplain /home/scudette/projects/rekall-test/osx10.9.5/TestExplain
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/osx10.9.5/output/TestExplain /home/scudette/projects/rekall-test/osx10.9.5/TestExplain
elif [ "$1" == "run" ]; then
