#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/osx10.9.5/output/TestDarwinIfnetCollector /home/scudette/projects/rekall-test/osx10.9.5/TestDarwinIfnetCollector
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/osx10.9.5/output/TestDarwinIfnetCollector /home/scudette/projects/rekall-test/osx10.9.5/TestDarwinIfnetCollector
