#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/adam/src/rekall-test/osx10.9.5/output/TestSearch /home/adam/src/rekall-test/osx10.9.5/TestSearch
elif [ "$1" == "meld" ]; then
   meld /home/adam/src/rekall-test/osx10.9.5/output/TestSearch /home/adam/src/rekall-test/osx10.9.5/TestSearch
elif [ "$1" == "diff" ]; then
   diff /home/adam/src/rekall-test/osx10.9.5/output/TestSearch /home/adam/src/rekall-test/osx10.9.5/TestSearch
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/adam/src/rekall-test/osx10.9.5/../profile_cache/' --format 'test' --filename '/home/adam/src/rekall-test/osx10.9.5/vmem.e01' --name_resolution_strategies 'Module' 'Symbol' - search 'select * from pslist() where (proc.pid == 1)'
else
   less /home/adam/src/rekall-test/osx10.9.5/output/TestSearch
fi
exit 0
