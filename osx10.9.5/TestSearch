#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/osx10.9.5/output/TestSearch /home/scudette/projects/rekall-test/osx10.9.5/TestSearch
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/osx10.9.5/output/TestSearch /home/scudette/projects/rekall-test/osx10.9.5/TestSearch
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/osx10.9.5/output/TestSearch /home/scudette/projects/rekall-test/osx10.9.5/TestSearch
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/scudette/projects/rekall-test/osx10.9.5/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/osx10.9.5/vmem.e01' --name_resolution_strategies 'Module' 'Symbol' - search 'select * from pslist() where (proc.pid == 1)'
else
   less /home/scudette/projects/rekall-test/osx10.9.5/output/TestSearch
fi
exit 0
