#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/scudette/projects/rekall-test/xen/output/TestLinMemDump /Users/scudette/projects/rekall-test/xen/TestLinMemDump
elif [ "$1" == "meld" ]; then
   meld /Users/scudette/projects/rekall-test/xen/output/TestLinMemDump /Users/scudette/projects/rekall-test/xen/TestLinMemDump
elif [ "$1" == "diff" ]; then
   diff /Users/scudette/projects/rekall-test/xen/output/TestLinMemDump /Users/scudette/projects/rekall-test/xen/TestLinMemDump
elif [ "$1" == "run" ]; then
   rekall  -v --cache_dir '/Users/scudette/projects/rekall-test/xen/../profile_cache/' --format 'test' --filename '/Users/scudette/projects/rekall-test/xen/xen-guest.E01' - memdump --proc_regex bash --dump_dir /var/folders/y8/670pn1dx55x01z5fs_6r2gnh003jp1/T/tmpKSnHwW/TestLinMemDumpThread-2
else
   less /Users/scudette/projects/rekall-test/xen/output/TestLinMemDump
fi
exit 0
