#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/xen/output/TestSearch /home/scudette/projects/rekall-test/xen/TestSearch
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/xen/output/TestSearch /home/scudette/projects/rekall-test/xen/TestSearch
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/xen/output/TestSearch /home/scudette/projects/rekall-test/xen/TestSearch
elif [ "$1" == "run" ]; then
