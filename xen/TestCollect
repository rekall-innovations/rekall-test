#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/xen/output/TestCollect /home/scudette/projects/rekall-test/xen/TestCollect
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/xen/output/TestCollect /home/scudette/projects/rekall-test/xen/TestCollect
