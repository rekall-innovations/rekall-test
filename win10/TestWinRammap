   cp /home/scudette/projects/rekall-test/win10/output/TestWinRammap /home/scudette/projects/rekall-test/win10/TestWinRammap
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/win10/output/TestWinRammap /home/scudette/projects/rekall-test/win10/TestWinRammap
