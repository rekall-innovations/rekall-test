#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/mic/projects/rekall-test/tigger/output/TestGrep /home/mic/projects/rekall-test/tigger/TestGrep
elif [ "$1" == "meld" ]; then
   meld /home/mic/projects/rekall-test/tigger/output/TestGrep /home/mic/projects/rekall-test/tigger/TestGrep
elif [ "$1" == "diff" ]; then
   diff /home/mic/projects/rekall-test/tigger/output/TestGrep /home/mic/projects/rekall-test/tigger/TestGrep
elif [ "$1" == "run" ]; then
   rekall  -v --format 'test' --filename '/home/mic/projects/rekall-test/tigger/tigger.vmem.E01' --cache_dir '/home/mic/projects/rekall-test/tigger/../profile_cache/' --cache 'memory' --name_resolution_strategies 'Module' 'Symbol' - grep explorer.exe --offset 4281886160
else
   less /home/mic/projects/rekall-test/tigger/output/TestGrep
fi
exit 0
