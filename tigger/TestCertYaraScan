#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/tigger/output/TestCertYaraScan /home/scudette/projects/rekall-test/tigger/TestCertYaraScan
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/tigger/output/TestCertYaraScan /home/scudette/projects/rekall-test/tigger/TestCertYaraScan
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/tigger/output/TestCertYaraScan /home/scudette/projects/rekall-test/tigger/TestCertYaraScan
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/scudette/projects/rekall-test/tigger/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/tigger/tigger.vmem.E01' --name_resolution_strategies 'Module' 'Symbol' - certscan --limit 20000000
else
   less /home/scudette/projects/rekall-test/tigger/output/TestCertYaraScan
fi
exit 0
