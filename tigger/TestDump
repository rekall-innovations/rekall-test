#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/mic/projects/rekall-test/tigger/output/TestDump /home/mic/projects/rekall-test/tigger/TestDump
elif [ "$1" == "meld" ]; then
   meld /home/mic/projects/rekall-test/tigger/output/TestDump /home/mic/projects/rekall-test/tigger/TestDump
elif [ "$1" == "diff" ]; then
   diff /home/mic/projects/rekall-test/tigger/output/TestDump /home/mic/projects/rekall-test/tigger/TestDump
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/mic/projects/rekall-test/tigger/../profile_cache/' --name_resolution_strategies 'Module' 'Symbol' --filename '/home/mic/projects/rekall-test/tigger/tigger.vmem.E01' --format 'test' - dump "nt!str:PoolTag" --length=100
else
   less /home/mic/projects/rekall-test/tigger/output/TestDump
fi
exit 0
