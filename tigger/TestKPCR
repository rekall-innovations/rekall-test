#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/mic/projects/rekall-test/tigger/output/TestKPCR /home/mic/projects/rekall-test/tigger/TestKPCR
elif [ "$1" == "meld" ]; then
   meld /home/mic/projects/rekall-test/tigger/output/TestKPCR /home/mic/projects/rekall-test/tigger/TestKPCR
elif [ "$1" == "diff" ]; then
   diff /home/mic/projects/rekall-test/tigger/output/TestKPCR /home/mic/projects/rekall-test/tigger/TestKPCR
elif [ "$1" == "run" ]; then
   rekall  -v --format 'test' --name_resolution_strategies 'Module' 'Symbol' --filename '/home/mic/projects/rekall-test/tigger/tigger.vmem.E01' --cache_dir '/home/mic/projects/rekall-test/tigger/../profile_cache/' --cache 'memory' - kpcr
else
   less /home/mic/projects/rekall-test/tigger/output/TestKPCR
fi
exit 0
