#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/adamsh/Projects/rekall-test/tigger/output/TestWin32kAutodetect /Users/adamsh/Projects/rekall-test/tigger/TestWin32kAutodetect
elif [ "$1" == "meld" ]; then
   meld /Users/adamsh/Projects/rekall-test/tigger/output/TestWin32kAutodetect /Users/adamsh/Projects/rekall-test/tigger/TestWin32kAutodetect
