elif [ "$1" == "run" ]; then
   rekall  --renderer 'test' --filename '/Users/adamsh/Projects/rekall-test/tigger/tigger.vmem.E01' --name_resolution_strategies 'Module,Symbol' - win32k_autodetect
else
   less /Users/adamsh/Projects/rekall-test/tigger/output/TestWin32kAutodetect
fi
exit 0
