#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/scudette/projects/rekall-test/tigger/output/TestBuildProfileLocally /Users/scudette/projects/rekall-test/tigger/TestBuildProfileLocally
elif [ "$1" == "meld" ]; then
   meld /Users/scudette/projects/rekall-test/tigger/output/TestBuildProfileLocally /Users/scudette/projects/rekall-test/tigger/TestBuildProfileLocally
elif [ "$1" == "diff" ]; then
   diff /Users/scudette/projects/rekall-test/tigger/output/TestBuildProfileLocally /Users/scudette/projects/rekall-test/tigger/TestBuildProfileLocally
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/Users/scudette/projects/rekall-test/tigger/../profile_cache/' --format 'test' --filename '/Users/scudette/projects/rekall-test/tigger/tigger.vmem.E01' --name_resolution_strategies 'Module' 'Symbol' - build_local_profile ntkrnlpa BD8F451F3E754ED8A34B50560CEB08E31 --dumpfile /var/folders/y8/670pn1dx55x01z5fs_6r2gnh003jp1/T/tmpxodq6M/TestBuildProfileLocallyThread-3/output
else
   less /Users/scudette/projects/rekall-test/tigger/output/TestBuildProfileLocally
fi
exit 0
