#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/tigger/output/TestLargePagePTE /home/scudette/projects/rekall-test/tigger/TestLargePagePTE
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/tigger/output/TestLargePagePTE /home/scudette/projects/rekall-test/tigger/TestLargePagePTE
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/tigger/output/TestLargePagePTE /home/scudette/projects/rekall-test/tigger/TestLargePagePTE
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/scudette/projects/rekall-test/tigger/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/tigger/tigger.vmem.E01' --name_resolution_strategies 'Module' 'Symbol' - run --run /tmp/tmpyvGzC8/TestLargePagePTE.script
else
   less /home/scudette/projects/rekall-test/tigger/output/TestLargePagePTE
fi
exit 0
