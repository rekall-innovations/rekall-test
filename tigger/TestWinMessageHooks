#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /Users/adamsh/Projects/rekall-test/tigger/output/TestWinMessageHooks /Users/adamsh/Projects/rekall-test/tigger/TestWinMessageHooks
elif [ "$1" == "meld" ]; then
   meld /Users/adamsh/Projects/rekall-test/tigger/output/TestWinMessageHooks /Users/adamsh/Projects/rekall-test/tigger/TestWinMessageHooks
elif [ "$1" == "diff" ]; then
   diff /Users/adamsh/Projects/rekall-test/tigger/output/TestWinMessageHooks /Users/adamsh/Projects/rekall-test/tigger/TestWinMessageHooks
elif [ "$1" == "run" ]; then
   rekall  --renderer 'test' --filename '/Users/adamsh/Projects/rekall-test/tigger/tigger.vmem.E01' --name_resolution_strategies 'Module,Symbol' - messagehooks
else
   less /Users/adamsh/Projects/rekall-test/tigger/output/TestWinMessageHooks
fi
exit 0
