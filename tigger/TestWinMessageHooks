#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/tigger/output/TestWinMessageHooks /home/scudette/projects/rekall-test/tigger/TestWinMessageHooks
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/tigger/output/TestWinMessageHooks /home/scudette/projects/rekall-test/tigger/TestWinMessageHooks
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/tigger/output/TestWinMessageHooks /home/scudette/projects/rekall-test/tigger/TestWinMessageHooks
elif [ "$1" == "run" ]; then
   rekall  --renderer 'test' --filename '/home/scudette/projects/rekall-test/tigger/tigger.vmem.E01' --name_resolution_strategies 'Module,Symbol' - messagehooks
else
   less /home/scudette/projects/rekall-test/tigger/output/TestWinMessageHooks
fi
exit 0
