   diff /home/mic/projects/rekall-test/tigger/output/TestSearch /home/mic/projects/rekall-test/tigger/TestSearch
elif [ "$1" == "run" ]; then
   rekall  -v --format 'test' --name_resolution_strategies 'Module' 'Symbol' --filename '/home/mic/projects/rekall-test/tigger/tigger.vmem.E01' --cache_dir '/home/mic/projects/rekall-test/tigger/../profile_cache/' --cache 'memory' - search 'select * from pslist() where _EPROCESS.pid == 4'
else
   less /home/mic/projects/rekall-test/tigger/output/TestSearch
fi
exit 0
