elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/adam/src/rekall-test/tigger/../profile_cache/' --format 'test' --filename '/home/adam/src/rekall-test/tigger/tigger.vmem.E01' --name_resolution_strategies 'Module' 'Symbol' - search 'select * from pslist where _EPROCESS.pid == 4'
else
   less /home/adam/src/rekall-test/tigger/output/TestSearch
fi
exit 0
