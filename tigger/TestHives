#!/bin/bash
if [ "$1" == "ok" ]; then
   cp /home/scudette/projects/rekall-test/tigger/output/TestHives /home/scudette/projects/rekall-test/tigger/TestHives
elif [ "$1" == "meld" ]; then
   meld /home/scudette/projects/rekall-test/tigger/output/TestHives /home/scudette/projects/rekall-test/tigger/TestHives
elif [ "$1" == "diff" ]; then
   diff /home/scudette/projects/rekall-test/tigger/output/TestHives /home/scudette/projects/rekall-test/tigger/TestHives
elif [ "$1" == "run" ]; then
   rekall  -v --cache 'memory' --cache_dir '/home/scudette/projects/rekall-test/tigger/../profile_cache/' --format 'test' --filename '/home/scudette/projects/rekall-test/tigger/tigger.vmem.E01' --name_resolution_strategies 'Module' 'Symbol' - hives
else
   less /home/scudette/projects/rekall-test/tigger/output/TestHives
fi
exit 0
